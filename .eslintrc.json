{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "next/core-web-vitals",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {
        "quotes": ["error", "single"],
        "semi": "error",
        "@next/next/no-img-element": "off"
    }
}
